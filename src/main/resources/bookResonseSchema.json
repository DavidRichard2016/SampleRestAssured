  "required": [
    "rating",
    "subtitle",
    "author",
    "pubdate",
    "tags",
    "origin_title",
    "image",
    "binding",
    "translator",
    "catalog",
    "pages",
    "images",
    "alt",
    "id",
    "publisher",
    "isbn10",
    "isbn13",
    "title",
